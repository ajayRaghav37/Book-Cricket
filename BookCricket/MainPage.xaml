        <TextBlock x:Name="lblToss" HorizontalAlignment="Left" Margin="850,28,0,0" Grid.Row="1" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="22" FontFamily="Segoe UI" Width="437" Height="29"/>
