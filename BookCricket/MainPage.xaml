        <Button x:Name="btnBowl" Content="BOWLING" HorizontalAlignment="Left" Height="57" Margin="1038,89,0,0" Grid.Row="1" VerticalAlignment="Top" Width="180" FontSize="20" IsEnabled="False" Click="btnBowl_Click"/>
